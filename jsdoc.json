{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.ts$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/backend",
    "recurse": true,
    "verbose": true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
